<div class="content-box">
	<div class="content-box-header">
		<h3>查看装箱明细</h3>
		<div class="operations">
			<%= link_to '编辑', edit_declaration_packing_path(@declaration_packing) %> |
			<%= link_to '返回', declaration_packings_path(:declaration_id => @declaration.id) %>
		</div>
	</div>
	<div class="content-box-content">
		<div class="tab-content default-tab">
			<p>
			  <b>商品名称:</b>
			  <%= @declaration_packing.name %>
			</p>
			
			<p>
			  <b>箱(件)数:</b>
			  <%= @declaration_packing.package_amount %>
			</p>
			
			<p>
			  <b>包装数量:</b>
			  <%= @declaration_packing.quantity %>
			</p>
			
			<p>
			  <b>申报单位:</b>
			  <span class="autoload" data-url=<%= dict_units_path %> ><%= @declaration_packing.unit %></span>
			</p>
			
			<p>
			  <b>总毛重:</b>
			  <%= @declaration_packing.gross_weight %>
			</p>
			
			<p>
			  <b>总净重:</b>
			  <%= @declaration_packing.net_weight %>
			</p>
			
			<p>
			  <b>每箱毛重:</b>
			  <%= @declaration_packing.gross_weight_per_package %>
			</p>
			
			<p>
			  <b>每箱净重:</b>
			  <%= @declaration_packing.net_weight_per_package %>
			</p>			
		</div>
	</div>
</div>
