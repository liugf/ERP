<%= form_tag(eval("search_" + model.table_name + "_path"), :method => "get") do %>
<p>
	<label style="display: inline;" >搜索：</label>
	<input name="term" class="text-input medium-input" />
	&nbsp;&nbsp;
	<input class="button" type="submit" value="<%=  model.model_name.human == '付费信息' ? "录入企业付费明细" : '搜索' + model.model_name.human %>" name="commit">
</p>
<div class="clear"></div><!-- End .clear -->
<% end %>


