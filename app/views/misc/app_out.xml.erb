        <SIGN_DATE><%= now.strftime('%Y%m%d%H%M%S') rescue ''%></SIGN_DATE>
        <NOTE><%= @application.note %></NOTE>
    </FPT_APP_SIGN>
    <FPT_APP_OUT_HEAD>
        <APP_NO></APP_NO>
        <SEQ_NO></SEQ_NO>
        <APP_CLASS><%= @application.app_class ? "H":"Z" %></APP_CLASS>
        <COP_APP_NO><%= @application.cop_app_no %></COP_APP_NO>
        <MAST_CUST><%= @application.mast_cust %></MAST_CUST>
        <EMS_NO><%= @application.contract.manual %></EMS_NO>
        <TRADE_CODE><%= @application.trade_code %></TRADE_CODE>
        <TRADE_NAME><%= @application.enterprise.name %></TRADE_NAME>
        <AGENT_CODE><%= @application.agent_code %></AGENT_CODE>
        <AGENT_NAME><%= Enterprise.find_by_code(@application.agent_code).name %></AGENT_NAME>
        <CORP><%= @application.corp %></CORP>
        <DECL><%= @application.decl %></DECL>
        <DIST_CODE><%= @application.dict_code %></DIST_CODE>
        <CONTR_NO><%= @application.contr_no %></CONTR_NO>
        <LICE_NO><%= @application.lice_no %></LICE_NO>
        <D_DATE><%= @application.d_date.strftime('%Y%m%d') %></D_DATE>
        <NOTE><%= @application.in_note%></NOTE>
        <IN_TRADE_CODE><%= @application.in_trade_code %></IN_TRADE_CODE>
        <IN_TRADE_NAME><%= @application.in_enterprise.name %></IN_TRADE_NAME>
        <IN_DIST_CODE><%= @application.in_dict_code %></IN_DIST_CODE>
        <CONVEY_DAY><%= @application.convey_day %></CONVEY_DAY>
        <CONVEY_SPA><%= @application.convey_spa %></CONVEY_SPA>
        <DECLARE_TYPE>1</DECLARE_TYPE>
        <LW_MARK><%= @application.lw_mark %></LW_MARK>
        <END_DATE><%= @application.end_date.strftime('%Y%m%d') %></END_DATE>
    </FPT_APP_OUT_HEAD>
    <FPT_APP_LIST>
		<% @application.application_cargos.out.each_with_index do |application_cargo, i|%>
			<FPT_APP_ITEM>
				<LIST_NO><%= i %></LIST_NO>
				<IN_OUT_NO></IN_OUT_NO>
				<IN_EMS_NO></IN_EMS_NO>
				<TR_NO><%= application_cargo.no_in_contract%></TR_NO>
				<CODE_T_S><%= application_cargo.code%></CODE_T_S>
				<G_NAME><%= application_cargo.name%></G_NAME>
				<G_MODEL><%= application_cargo.specification %></G_MODEL>
				<UNIT><%= application_cargo.unit%></UNIT>
				<UNIT_1><%= application_cargo.unit1 %></UNIT_1>
				<QTY><%= application_cargo.quantity %></QTY>
				<QTY_1><%= application_cargo.quantity1%></QTY_1>
				<NOTE><%= application_cargo.note%></NOTE>
				<BAK_CODE_TS><%= application_cargo.bak_code_ts %></BAK_CODE_TS>
				<MODIFY_MARK>3</MODIFY_MARK>
			</FPT_APP_ITEM>
		<% end %>	
    </FPT_APP_LIST>
