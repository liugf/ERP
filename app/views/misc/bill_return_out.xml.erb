<% enterprise = @bill.enterprise %>
<% custom_option = enterprise.enterprise_custom_option %>
<% now = Time.now %><?xml version="1.0" encoding="UTF-8"?>
<FPT_DATA version="1.0" sendId="" receiveId="">
    <FPT_BILL_SIGN>
        <SEQ_NO></SEQ_NO>
        <APP_NO><%= @bill.application.app_no%></APP_NO>
        <SYS_TYPE>1</SYS_TYPE>
        <COP_BILL_NO><%= @bill.cop_bill_no%></COP_BILL_NO>
        <BILL_SORT>1</BILL_SORT>
        <ICCARD_ID><%= custom_option.ic_card_no rescue "" %></ICCARD_ID>
        <ICCARD_NAME>加贸平台</ICCARD_NAME>
        <ICCARD_UNIT_CODE><%= enterprise.organization_code %></ICCARD_UNIT_CODE>
        <ICCARD_UNIT_NAME><%= enterprise.name %></ICCARD_UNIT_NAME>
        <CERT_SEQ_NO><%= custom_option.certificate_no rescue "" %></CERT_SEQ_NO>
        <SIGN><%= @signed_data %></SIGN>
        <SIGN_DATE><%= now.strftime("%Y%m%d%H%M%S") rescue "" %></SIGN_DATE>
        <NOTE><%= @bill.note%></NOTE>
    </FPT_BILL_SIGN>
    <FPT_BILL_HEAD>
        <BILL_NO></BILL_NO>
        <SEQ_NO></SEQ_NO>
        <APP_NO><%= @bill.application.app_no%></APP_NO>
        <COP_BILL_NO><%= @bill.cop_bill_no%></COP_BILL_NO>
        <TRADE_CODE><%= @bill.trade_code%></TRADE_CODE>
        <TRADE_NAME><%= @bill.enterprise.name %></TRADE_NAME>
        <ISSUE_DATE><%= @bill.issue_date %></ISSUE_DATE>
        <IS_DECLA_EM><%= @bill.is_decla_em %></IS_DECLA_EM>
        <IS_DECLA_DATE><%= @bill.is_decla_date %></IS_DECLA_DATE>
        <AGENT_CODE><%= @bill.agent_code %></AGENT_CODE>
        <AGENT_NAME><%= Enterprise.find_by_code(@bill.agent_code).name %></AGENT_NAME>
        <CONVEY_TYPE><%= @bill.convey_type %></CONVEY_TYPE>
        <CONVEY_NO><%= @bill.convey_no %></CONVEY_NO>
        <NOTE><%= @bill.note %></NOTE>
        <CONTR_NO><%= @bill.contr_no %></CONTR_NO>
    </FPT_BILL_HEAD>
	<% in_ems_no = @bill.application.ems_no rescue '' %>
    <FPT_BILL_LIST>
		<% @bill.bill_cargos.out.each_with_index do |bill_cargo , i| %>
			<FPT_BILL_ITEM>
				<LIST_NO><%= i%></LIST_NO>
				<OUT_NO></OUT_NO>
				<IN_EMS_NO><%= in_ems_no %></IN_EMS_NO>
				<COP_G_NO><%= bill_cargo.cop_g_no %></COP_G_NO>
				<COP_G_NAME><%= bill_cargo.before_name %></COP_G_NAME>
				<COP_G_MODEL><%= bill_cargo.before_specification %></COP_G_MODEL>
				<APP_G_NO><%= bill_cargo.no_in_application %></APP_G_NO>
				<G_NO><%= bill_cargo.no %></G_NO>
				<CODE_T_S><%= bill_cargo.code %></CODE_T_S>
				<G_NAME><%= bill_cargo.name %></G_NAME>
				<G_MODEL><%= bill_cargo.specification %></G_MODEL>
				<TRADE_UNIT><%= bill_cargo.unit %></TRADE_UNIT>
				<TRADE_QTY><%= bill_cargo.quantity %></TRADE_QTY>
				<UNIT><%= bill_cargo.unit1 %></UNIT>
				<QTY><%= bill_cargo.quantity1 %></QTY>
				<NOTE><%= bill_cargo.note %></NOTE>
			</FPT_BILL_ITEM>
		<% end %>
	</FPT_BILL_LIST>
    <FPT_BILL_DICT_LIST>
		<% @bill.bill_cargos.out.each_with_index do |bill_cargo , i| %>
			<FPT_BILL_DICT_ITEM>
				<LIST_NO><%= i%></LIST_NO>
				<OUT_NO></OUT_NO>
				<IN_EMS_NO><%= in_ems_no %></IN_EMS_NO>
				<COP_G_NO><%= bill_cargo.cop_g_no %></COP_G_NO>
				<COP_G_NAME><%= bill_cargo.before_name %></COP_G_NAME>
				<COP_G_MODEL><%= bill_cargo.before_specification %></COP_G_MODEL>
				<APP_G_NO><%= bill_cargo.no_in_application %></APP_G_NO>
				<G_NO><%= bill_cargo.no %></G_NO>
				<CODE_T_S><%= bill_cargo.code %></CODE_T_S>
				<G_NAME><%= bill_cargo.name %></G_NAME>
				<G_MODEL><%= bill_cargo.specification %></G_MODEL>
				<TRADE_UNIT><%= bill_cargo.unit %></TRADE_UNIT>
				<TRADE_QTY><%= bill_cargo.quantity %></TRADE_QTY>
				<UNIT><%= bill_cargo.unit1 %></UNIT>
				<QTY><%= bill_cargo.quantity1 %></QTY>
				<NOTE><%= bill_cargo.note %></NOTE>
			</FPT_BILL_DICT_ITEM>
		<% end %>
    </FPT_BILL_DICT_LIST>
</FPT_DATA>