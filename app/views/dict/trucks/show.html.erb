<div class="content-box">
	<div class="content-box-header">
		<h3>查看<%= @model_name %></h3>
		<div class="operations">
			<%= link_to '编辑', edit_dict_truck_path(@item) %> |
			<%= link_to '返回列表', dict_trucks_path %>
		</div>
	</div>
	<div class="content-box-content">
		<div class="tab-content default-tab">
			<p>
				<b>编码</b>：<%= @item.code %>
			</p>
			<p>
				<b>大陆车牌</b>：<%= @item.name %>
			</p>
			<p>
				<b>香港车牌</b>：<%= @item.hk_truck_no %>
			</p>
			<p>
				<b>IC卡</b>：<%= @item.ic_card %>
			</p>
			<p>
				<b>承运单位名称</b>：<%= @item.corporation_name %>
			</p>
			<p>
				<b>承运单位组织机构代码</b>：<%= @item.corporation_code %>
			</p>
			<p>
				<b>承运单位地址</b>：<%= @item.corporation_address %>
			</p>
			<p>
				<b>承运单位电话</b>：<%= @item.corporation_telephone %>
			</p>
			<p>
				<b>司机姓名</b>：<%= @item.driver_name %>
			</p>
		</div>
	</div>
</div>
