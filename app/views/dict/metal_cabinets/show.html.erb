<div class="content-box">
	<div class="content-box-header">
		<h3>查看<%= @model_name %></h3>
		<div class="operations">
			<%= link_to '编辑', edit_dict_metal_cabinet_path(@item) %> |
			<%= link_to '返回列表', dict_metal_cabinets_path %>
		</div>
	</div>
	<div class="content-box-content">
		<div class="tab-content default-tab">
			<p>
				<b>编码</b>：<%= @item.code %>
			</p>
			<p>
				<b>名称</b>：<%= @item.name %>
			</p>
			<p>
				<b>重量</b>：<%= @item.weight %>
			</p>
			<p>
				<b>尺寸/b>：<%= @item.size %>
			</p>
		</div>
	</div>
</div>
