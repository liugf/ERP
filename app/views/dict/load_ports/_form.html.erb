<%= render "shared/error_messages", :model => @item %>
<%= form_for(@item) do |f| %>
<fieldset>
	<p>
		<label>编码</label>
		<%= f.text_field :code, :class => "text-input small-input first-field" %>
		<br />
	</p>
	<p>
		<label>名称</label>
		<%= f.text_field :name, :class => "text-input small-input" %>
		<br />
	</p>
	<p>
		<label>海关编码</label>
		<%= render "shared/autocomplete_field", :form => f, :attribute => :custom_code, :url => dict_customs_path, :klass => "text-input small-input" %> 
		<br />
	</p>
	<p>
		<%= f.submit :class => "button" %>
	</p>
</fieldset>
<div class="clear"></div><!-- End .clear -->
<% end %>

