<div class="content-box">
	<div class="content-box-header">
		<h3>查看<%= @model_name %></h3>
		<div class="operations">
			<%= link_to '编辑', edit_dict_good_path(@item) %> |
			<%= link_to '返回列表', dict_goods_path %>
		</div>
	</div>
	<div class="content-box-content">
		<div class="tab-content default-tab">
			<p>
				<b>编码</b>：<%= @item.code %>
			</p>
			<p>
				<b>名称</b>：<%= @item.name %>
			</p>
			<p>
				<b>单位1</b>：<span class="autoload" data-url=<%= dict_units_path %> ><%= @item.unit1 %></span>
			</p>
			<p>
				<b>单位2</b>：<span class="autoload" data-url=<%= dict_units_path %> ><%= @item.unit2 %></span>
			</p>
			<p>
				<b>备注</b>：<%= @item.memo %>
			</p>
		</div>
	</div>
</div>
