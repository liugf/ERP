<%= render "shared/error_messages", :model => @item %>
<%= form_for(@item) do |f| %>
<fieldset>
	<p>
		<label for="dict_custom_code">编码</label>
		<%= f.text_field :code, :class => "text-input small-input" %>
		<br />
	</p>
	<p>
		<label for="dict_custom_name">名称</label>
		<%= f.text_field :name, :class => "text-input medium-input" %>
		<br />
	</p>
	<p>
		<input class="button" type="submit" value="确定" name="submit">
	</p>
</fieldset>
<div class="clear"></div><!-- End .clear -->
<% end %>

